{
  "name": "ra-ng-wip",
  "version": "2.0.0",
  "description": "Work in progress ra-ng 2.x",
  "scripts": {
    "start": "rimraf dist && tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c liteserver.json",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc":"ngc -p tsconfig-aot.json",
    "tslint": "tslint -c tslint.json src/app/**/*.ts",
    "test": "rimraf dist && tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "rimraf dist && tsc && karma start karma.conf.js --single-run",
    "e2e": "protractor protractor.config.js",
    "webdriver:update": "webdriver-manager update",
    "karma": "karma start karma.conf.js"
  },
  "keywords": [
    "angular2",
    "ra-ng",
    "systemjs",
    "aot"
  ],
  "author": "ICT Analysis, Architecture & Design",
  "license": "Apache-2.0",
  "dependencies": {
